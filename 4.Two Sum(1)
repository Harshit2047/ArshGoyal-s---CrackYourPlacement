class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        int s=nums.size();
        vector<int>v;
        for(int i=0;i<s;i++){
            int c=0;
            for(int j=i+1;j<s;j++){
                int x=nums[i]+nums[j];
                if(x==target){
                    v.push_back(i);
                    v.push_back(j);
                   return v;
                    
                }
                
            }
            
        }
        return v;
    }
};
